#include "WProgram.h"

void setup()
{
	pinMode(13, OUTPUT);
	analogWriteResolution(8);	// 8bit/Resolution
}

void loop()
{
	if (usb_audio_receive_buffer[0]<0x7fff) digitalWriteFast(13, LOW);
	else digitalWriteFast(13, HIGH);
	analogWrite(9, 50);
}

extern "C" int main()
{
	setup();
	while (1) {
		loop();
		yield();
	}
}

